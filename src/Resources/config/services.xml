<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="MastoxExtendedDocuments\Services\Content\Media\Metadata\MetadataLoader\DocumentMetaDataLoader">
            <tag name="shopware.metadata.loader"/>
        </service>


        <service id="MastoxExtendedDocuments\Services\Content\Media\TypeDetector\DocumentTypeExtensionDetector">
            <tag name="shopware.media_type.detector" priority="1"/>
        </service>

        <service id="MastoxExtendedDocuments\Entity\Downloads\ProductDownloadDefinition">
            <tag name="shopware.entity.definition" entity="product_downloads"/>
        </service>

        <service id="MastoxExtendedDocuments\Extension\Media\MediaExtension">
            <tag name="shopware.entity.extension"/>
        </service>

        <service id="MastoxExtendedDocuments\Extension\Product\ProductExtension">
            <tag name="shopware.entity.extension"/>
        </service>
    </services>


</container>
